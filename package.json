{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "start": "concurrently \"php artisan serve\" \"npm run dev\"",
        "production": "npm run build && php artisan serve",
        "clean": "rm -rf node_modules && npm install",
        "clean-cache": "php artisan cache:clear && php artisan config:clear && php artisan view:clear"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "@inertiajs/react": "^2.1.1",
        "@vitejs/plugin-react": "^4.7.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    }
}
